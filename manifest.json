{
	"name": "Vue Component Finder",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Code locate chrome plugin.",
	"content_scripts": [
		{
			"matches": ["http://*/*","https://*/*"],
			"js": ["index.js"],
			"run_at":"document_end"
		}
	],
    "web_accessible_resources": [
    	"index.css",
    	"highlight.css"
    ]
}